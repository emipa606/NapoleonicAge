<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <FactionDef ParentName="FactionBase">
    <defName>France</defName>
    <label>French Regiment</label>
    <description>A French outpost. Most outposts are assigned a regiment for garrison, and this regiment acts as both a defense and an offense in the case of attacking enemy factions.</description>
    <pawnsPlural>French</pawnsPlural>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <maxConfigurableAtWorldCreation>1</maxConfigurableAtWorldCreation>
    <settlementGenerationWeight>.5</settlementGenerationWeight>
    <canSiege>true</canSiege>
    <factionIconPath>Things/France/Items/Home</factionIconPath>
    <canStageAttacks>true</canStageAttacks>
    <leaderTitle>Colonel</leaderTitle>
    <caravanTraderKinds>
      <li>Caravan_France_BulkGoods</li>
      <li>Caravan_France_CombatSupplier</li>
    </caravanTraderKinds>
    <visitorTraderKinds>
      <li>Visitor_France_Standard</li>
    </visitorTraderKinds>
    <baseTraderKinds>
      <li>FactionBase_France_Standard</li>
    </baseTraderKinds>
    <factionNameMaker>NamerFactionFrance</factionNameMaker>
    <techLevel>Industrial</techLevel>
    <backstoryFilters>
      <li>
        <categories>
          <li>Outlander</li>
        </categories>
      </li>
    </backstoryFilters>
    <allowedCultures>
      <li>Rustican</li>
    </allowedCultures>
    <maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,35)</li>
        <li>(70, 50)</li>
        <li>(700, 100)</li>
        <li>(1300, 150)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <pawnGroupMakers>
      <li>
        <kindDef>Combat</kindDef>
        <options>
          <FrenchLine>100</FrenchLine>
          <FrenchMilitia>30</FrenchMilitia>
          <FrenchLight>50</FrenchLight>
          <FrenchCommand>1</FrenchCommand>
        </options>
      </li>
      <li>
        <kindDef>Trader</kindDef>
        <traders>
          <Town_Trader>1</Town_Trader>
        </traders>
        <carriers>
          <Muffalo>1</Muffalo>
          <Dromedary>1</Dromedary>
        </carriers>
        <guards>
          <FrenchLine>100</FrenchLine>
          <FrenchMilitia>30</FrenchMilitia>
          <FrenchLight>50</FrenchLight>
          <FrenchCommand>1</FrenchCommand>
        </guards>
      </li>
      <li>
        <kindDef>Settlement</kindDef>
        <options>
          <FrenchLine>100</FrenchLine>
          <FrenchMilitia>30</FrenchMilitia>
          <FrenchLight>50</FrenchLight>
          <FrenchCommand>1</FrenchCommand>
        </options>
      </li>
      <li>
        <kindDef>Peaceful</kindDef>
        <options>
          <FrenchLine>100</FrenchLine>
          <FrenchMilitia>30</FrenchMilitia>
          <FrenchLight>50</FrenchLight>
          <FrenchCommand>1</FrenchCommand>
        </options>
      </li>
    </pawnGroupMakers>
    <allowedArrivalTemperatureRange>
      <min>-5</min>
      <max>45</max>
    </allowedArrivalTemperatureRange>
    <settlementTexturePath>Things/France/Items/Home</settlementTexturePath>
  </FactionDef>
